{
  "name": "@salesforce/source-tracking",
  "description": "API for tracking local and remote Salesforce metadata changes",
  "version": "2.2.14",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/forcedotcom/source-tracking#readme",
  "repository": "forcedotcom/source-tracking",
  "scripts": {
    "build": "sf-build",
    "ci-docs": "yarn sf-ci-docs",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "yarn sf-lint --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "prune:dead": "ts-prune | grep -v 'source-deploy-retrieve' | grep -v 'index.ts'",
    "test": "sf-test",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "test:nuts:local": "mocha \"**/local/*.nut.ts\" --slow 4500 --timeout 600000 --parallel"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "source",
    "tracking"
  ],
  "files": [
    "docs",
    "lib",
    "messages",
    "!lib/**/*.map",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@salesforce/core": "^3.32.1",
    "@salesforce/kit": "^1.8.0",
    "@salesforce/source-deploy-retrieve": "^7.5.12",
    "graceful-fs": "^4.2.10",
    "isomorphic-git": "1.17.0",
    "ts-retry-promise": "^0.7.0"
  },
  "devDependencies": {
    "@salesforce/cli-plugins-testkit": "^2.5.3",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.4.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "chai": "^4.3.7",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "sinon": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.9.3"
  },
  "config": {},
  "publishConfig": {
    "access": "public"
  }
}