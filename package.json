{
  "name": "plugin-template",
  "description": "A template repository for sfdx plugins",
  "version": "1.0.0",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/config": "^1",
    "@salesforce/command": "^2.2.0",
    "@salesforce/core": "^2.6.0",
    "@salesforce/ts-sinon": "1.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/dev-scripts": "0.6.1",
    "@types/chai": "^4",
    "chai": "^4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "mocha": "^5",
    "nyc": "15.0.1",
    "prettier": "1.16.4",
    "shx": "0.2.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-template",
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "repository": "salesforcecli/plugin-template",
  "scripts": {
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "tsc -b",
    "build": "yarn run clean:lib && yarn compile",
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn run build && oclif-dev manifest",
    "reformat": "prettier --config .prettierrc --write './*.{js,json,md}' './**/*.{ts,json,md}'",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "yarn sf-husky-commit-msg"
    }
  }
}
