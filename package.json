{
  "name": "@salesforce/source-tracking",
  "description": "API for tracking local and remote Salesforce metadata changes",
  "version": "0.0.1",
  "author": "Salesforce",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/forcedotcom/source-tracking#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/forcedotcom/source-tracking.git"
  },
  "scripts": {
    "build": "sf-build",
    "ci-docs": "yarn sf-ci-docs",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "yarn sf-lint --fix",
    "prepack": "sf-build",
    "pretest": "sf-compile-test",
    "test": "sf-test --require ts-node/register",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "source",
    "tracking"
  ],
  "files": [
    "docs",
    "lib",
    "messages",
    "!lib/**/*.map"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "dependencies": {
    "@oclif/config": "^1.17.0",
    "@salesforce/command": "^4.0.0",
    "@salesforce/core": "^2.27.0",
    "@salesforce/kit": "^1.5.17",
    "isomorphic-git": "^1.9.2"
  },
  "devDependencies": {
    "@salesforce/cli-plugins-testkit": "^1.2.12",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.18",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sinon": "^11.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
